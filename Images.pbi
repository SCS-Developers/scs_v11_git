Procedure IMG_drawAudGraphMarkers()
  PROCNAMEC()
  
  gnNextImageNo + 1
  hAudGraphLevel = gnNextImageNo
  CreateImage(hAudGraphLevel, 8, 8)
  If StartDrawing(ImageOutput(hAudGraphLevel))
    Box(0,0,OutputWidth(),OutputHeight(),#SCS_Level_Color)
    ; draw letter L
    LineXY(2,1,2,6,#SCS_Black)
    LineXY(2,6,5,6,#SCS_Black)
    StopDrawing()
  EndIf
  
  gnNextImageNo + 1
  hAudGraphLevelSelected = gnNextImageNo
  CreateImage(hAudGraphLevelSelected, 8, 8)
  If StartDrawing(ImageOutput(hAudGraphLevelSelected))
    Box(0,0,OutputWidth(),OutputHeight(),#SCS_Black)
    ; draw letter L (larger than for hAudGraphLevel, and double-width)
    LineXY(1,0,1,7,#SCS_Level_Color)
    LineXY(2,0,2,7,#SCS_Level_Color)
    LineXY(1,6,6,6,#SCS_Level_Color)
    LineXY(2,7,6,7,#SCS_Level_Color)
    StopDrawing()
  EndIf
  
  gnNextImageNo + 1
  hAudGraphPan = gnNextImageNo
  CreateImage(hAudGraphPan, 8, 8)
  If StartDrawing(ImageOutput(hAudGraphPan))
    Box(0,0,OutputWidth(),OutputHeight(),#SCS_Pan_Color)
    ; draw letter P
    LineXY(2,1,2,6,#SCS_Black)
    Plot(3,1,#SCS_Black)
    Plot(3,4,#SCS_Black)
    Plot(4,1,#SCS_Black)
    Plot(4,4,#SCS_Black)
    Plot(5,1,#SCS_Light_Grey)
    Plot(5,2,#SCS_Black)
    Plot(5,3,#SCS_Black)
    Plot(5,4,#SCS_Light_Grey)
    StopDrawing()
  EndIf
  
  gnNextImageNo + 1
  hAudGraphCueMarker = gnNextImageNo
  CreateImage(hAudGraphCueMarker, 14, 12)
  If StartDrawing(ImageOutput(hAudGraphCueMarker))
    Box(0,0,OutputWidth(),OutputHeight(),#SCS_Level_Color)
    ; draw letters CM
    ; The Letter C in a square fashion
    LineXY(1,2,4,2, #SCS_Black)
    LineXY(1,2,1,10, #SCS_Black)
    LineXY(1,10,4,10, #SCS_Black)
    ; The Letter M
    LineXY(6,2,6,10, #SCS_Black)
    LineXY(6,2,9,10, #SCS_Black)
    LineXY(9,10,12,2, #SCS_Black)
    LineXY(12,2,12,10, #SCS_Black)    
    StopDrawing()
  EndIf
 
  gnNextImageNo + 1
  hAudGraphCuePoint = gnNextImageNo
  CreateImage(hAudGraphCuePoint, 14, 12)
  If StartDrawing(ImageOutput(hAudGraphCuePoint))
    Box(0,0,OutputWidth(),OutputHeight(),#SCS_Dark_Orange)
    ; draw letters CP
    ; The Letter C in a square fashion
    LineXY(1,2,4,2, #SCS_Black)
    LineXY(1,2,1,10, #SCS_Black)
    LineXY(1,10,4,10, #SCS_Black)
    ; The Letter P in a square fashion
    LineXY(7,2,7,10, #SCS_Black)
    LineXY(7,2,11,2, #SCS_Black)
    LineXY(11,2,11,7, #SCS_Black)
    LineXY(7,7,11,7, #SCS_Black)
    StopDrawing()
  EndIf
 
EndProcedure

Macro IMG_mac_drawTrBtnBackground()
  DrawingMode(#PB_2DDrawing_Gradient)      
  BackColor(nBackColorTop)
  FrontColor(nBackColorBottom)
  LinearGradient(0, 0, 0, OutputHeight())    
  Box(0,0,OutputWidth(),OutputHeight())
  FrontColor(nFrontColor)
  DrawingMode(#PB_2DDrawing_Default)
EndMacro

Procedure IMG_drawTrBtnImages13(pWidth, pHeight)
  ; draw or redraw transport button images for cue panels
  ; see also redrawCvsBtn()
  PROCNAMEC()
  Protected n, xBase, yBase, nWidth, nHeight
  Protected nBackColorTop, nBackColorBottom, nFrontColor
  Protected nImageNo
  
  nWidth = pWidth
  nHeight = pHeight
  
  ; check minimum size to prevent Plot() statement crashing
  If nWidth < 12
    nWidth = 12
  EndIf
  If nHeight < 10
    nHeight = 10
  EndIf
  
  If nWidth > 16
    xBase = (nWidth - 16) >> 1
  EndIf
  If nHeight > 14
    yBase = (nHeight - 14) >> 1
  EndIf
  
  If hTrPlayEn13 = 0
    ; allocate image numbers
    hTrPlayEn13 = gnNextImageNo + 1 ; "En" = Enabled
    hTrPlayDi13 = gnNextImageNo + 2 ; "Di" = Disabled
    hTrPlayMo13 = gnNextImageNo + 3 ; "Mo" = "Mouse over"
    hTrStopEn13 = gnNextImageNo + 4
    hTrStopDi13 = gnNextImageNo + 5
    hTrStopMo13 = gnNextImageNo + 6
    hTrPauseEn13 = gnNextImageNo + 7
    hTrPauseDi13 = gnNextImageNo + 8
    hTrPauseMo13 = gnNextImageNo + 9
    hTrRewindEn13 = gnNextImageNo + 10
    hTrRewindDi13 = gnNextImageNo + 11
    hTrRewindMo13 = gnNextImageNo + 12
    hTrFadeOutEn13 = gnNextImageNo + 13
    hTrFadeOutDi13 = gnNextImageNo + 14
    hTrFadeOutMo13 = gnNextImageNo + 15
    hTrReleaseEn13 = gnNextImageNo + 16
    hTrReleaseDi13 = gnNextImageNo + 17
    hTrReleaseMo13 = gnNextImageNo + 18
    hTrShuffleEn13 = gnNextImageNo + 19
    hTrShuffleDi13 = gnNextImageNo + 20
    hTrShuffleMo13 = gnNextImageNo + 21
    hTrFirstEn13 = gnNextImageNo + 22
    hTrFirstDi13 = gnNextImageNo + 23
    hTrFirstMo13 = gnNextImageNo + 24
    gnNextImageNo + 24
  EndIf
  
  For n = 0 To 2
    If n = 0
      ; enabled
      ; nb if these colors are changed then make the same changes in PNL_drawSwitch() in CuePanelControl.pbi
      nBackColorTop = RGB(246,246,246)
      nBackColorBottom = RGB(205,205,205)
      nFrontColor = $000000
    ElseIf n = 1
      ; disabled
      nBackColorTop = RGB(246,246,246)
      nBackColorBottom = RGB(205,205,205)
      nFrontColor = RGB(185,185,185)
    Else
      ; mouse over enabled
      nBackColorTop = RGB(223,251,255)
      nBackColorBottom = RGB(199,246,255)
      nFrontColor = $000000
    EndIf
    
    ; --------- play button image
    If n = 0
      nImageNo = hTrPlayEn13
    ElseIf n = 1
      nImageNo = hTrPlayDi13
    Else
      nImageNo = hTrPlayMo13
    EndIf
    If IsImage(nImageNo)
      FreeImage(nImageNo)
    EndIf
    CreateImage(nImageNo,nWidth,nHeight)
    If StartDrawing(ImageOutput(nImageNo))
      IMG_mac_drawTrBtnBackground()
      LineXY(xBase+5,yBase+1,xBase+5,yBase+11)
      LineXY(xBase+6,yBase+2,xBase+6,yBase+10)
      LineXY(xBase+7,yBase+3,xBase+7,yBase+9)
      LineXY(xBase+8,yBase+4,xBase+8,yBase+8)
      LineXY(xBase+9,yBase+5,xBase+9,yBase+7)
      Plot(xBase+10,yBase+6)
      If n <> 1
        LineXY(xBase+5,yBase+12,xBase+10,yBase+7,#SCS_Light_Grey)
      EndIf
      StopDrawing()
    EndIf
    
    ; --------- stop button image
    gnNextImageNo + 1
    If n = 0
      nImageNo = hTrStopEn13
    ElseIf n = 1
      nImageNo = hTrStopDi13
    Else
      nImageNo = hTrStopMo13
    EndIf
    If IsImage(nImageNo)
      FreeImage(nImageNo)
    EndIf
    CreateImage(nImageNo,nWidth,nHeight)
    If StartDrawing(ImageOutput(nImageNo))
      IMG_mac_drawTrBtnBackground()
      If n <> 1
        Box(xBase+5,yBase+3,9,9,#SCS_Light_Grey)
      EndIf
      Box(xBase+4,yBase+2,9,9)
      StopDrawing()
    EndIf
    
    ; --------- pause button image
    gnNextImageNo + 1
    If n = 0
      nImageNo = hTrPauseEn13
    ElseIf n = 1
      nImageNo = hTrPauseDi13
    Else
      nImageNo = hTrPauseMo13
    EndIf
    If IsImage(nImageNo)
      FreeImage(nImageNo)
    EndIf
    CreateImage(nImageNo,nWidth,nHeight)
    If StartDrawing(ImageOutput(nImageNo))
      IMG_mac_drawTrBtnBackground()
      Box(xBase+4,yBase+2,2,9)
      Box(xBase+9,yBase+2,2,9)
      StopDrawing()
    EndIf
    
    ; --------- rewind button image
    gnNextImageNo + 1
    If n = 0
      nImageNo = hTrRewindEn13
    ElseIf n = 1
      nImageNo = hTrRewindDi13
    Else
      nImageNo = hTrRewindMo13
    EndIf
    If IsImage(nImageNo)
      FreeImage(nImageNo)
    EndIf
    CreateImage(nImageNo,nWidth,nHeight)
    If StartDrawing(ImageOutput(nImageNo))
      IMG_mac_drawTrBtnBackground()
      Plot(xBase,yBase+6)
      LineXY(xBase+1,yBase+5,xBase+1,yBase+7)
      LineXY(xBase+2,yBase+4,xBase+2,yBase+8)
      LineXY(xBase+3,yBase+3,xBase+3,yBase+9)
      LineXY(xBase+4,yBase+2,xBase+4,yBase+10)
      LineXY(xBase+5,yBase+1,xBase+5,yBase+11)
      Plot(xBase+7,yBase+6)
      LineXY(xBase+8,yBase+5,xBase+8,yBase+7)
      LineXY(xBase+9,yBase+4,xBase+9,yBase+8)
      LineXY(xBase+10,yBase+3,xBase+10,yBase+9)
      LineXY(xBase+11,yBase+2,xBase+11,yBase+10)
      LineXY(xBase+12,yBase+1,xBase+12,yBase+11)
      StopDrawing()
    EndIf
    
    ; --------- fadeout button image
    gnNextImageNo + 1
    If n = 0
      nImageNo = hTrFadeOutEn13
    ElseIf n = 1
      nImageNo = hTrFadeOutDi13
    Else
      nImageNo = hTrFadeOutMo13
    EndIf
    If IsImage(nImageNo)
      FreeImage(nImageNo)
    EndIf
    CreateImage(nImageNo,nWidth,nHeight)
    If StartDrawing(ImageOutput(nImageNo))
      IMG_mac_drawTrBtnBackground()
      If n <> 1
        LineXY(xBase+2,yBase+4,xBase+12,yBase+8,#SCS_Light_Grey)
        LineXY(xBase+2,yBase+12,xBase+12,yBase+8,#SCS_Light_Grey)
      EndIf
      LineXY(xBase+2,yBase+2,xBase+12,yBase+6)
      LineXY(xBase+2,yBase+10,xBase+12,yBase+6)
      LineXY(xBase+2,yBase+3,xBase+12,yBase+7)
      LineXY(xBase+2,yBase+11,xBase+12,yBase+7)
      StopDrawing()
    EndIf
    
    ; --------- release button image
    gnNextImageNo + 1
    If n = 0
      nImageNo = hTrReleaseEn13
    ElseIf n = 1
      nImageNo = hTrReleaseDi13
    Else
      nImageNo = hTrReleaseMo13
    EndIf
    If IsImage(nImageNo)
      FreeImage(nImageNo)
    EndIf
    CreateImage(nImageNo,nWidth,nHeight)
    If StartDrawing(ImageOutput(nImageNo))
      IMG_mac_drawTrBtnBackground()
      Box(xBase+3,yBase+1,6,2)
      Box(xBase+2,yBase+2,2,2)
      Box(xBase+8,yBase+2,2,2)
      Box(xBase+1,yBase+3,2,6)
      Box(xBase+9,yBase+3,2,3)
      LineXY(xBase+3,yBase+5,xBase+3,yBase+6)
      Box(xBase+8,yBase+5,2,2)
      Box(xBase+4,yBase+6,5,2)
      Box(xBase+2,yBase+7,2,2)
      Box(xBase+3,yBase+9,11,2)
      LineXY(xBase+11,yBase+7,xBase+11,yBase+12)
      LineXY(xBase+12,yBase+8,xBase+12,yBase+11)
      StopDrawing()
    EndIf
    
    ; --------- shuffle button image
    gnNextImageNo + 1
    If n = 0
      nImageNo = hTrShuffleEn13
    ElseIf n = 1
      nImageNo = hTrShuffleDi13
    Else
      nImageNo = hTrShuffleMo13
    EndIf
    If IsImage(nImageNo)
      FreeImage(nImageNo)
    EndIf
    CreateImage(nImageNo,nWidth,nHeight)
    If StartDrawing(ImageOutput(nImageNo))
      IMG_mac_drawTrBtnBackground()
      Box(xBase+0,yBase+1,4,2)
      Box(xBase+0,yBase+9,4,2)
      Box(xBase+4,yBase+3,2,2)
      Box(xBase+4,yBase+8,2,2)
      Box(xBase+5,yBase+5,3,3)
      Box(xBase+7,yBase+3,2,2)
      Box(xBase+7,yBase+8,2,2)
      Box(xBase+9,yBase+2,6,2)
      Box(xBase+9,yBase+9,6,2)
      LineXY(xBase+12,yBase+0,xBase+12,yBase+5)
      LineXY(xBase+12,yBase+7,xBase+12,yBase+12)
      LineXY(xBase+13,yBase+1,xBase+13,yBase+4)
      LineXY(xBase+13,yBase+8,xBase+13,yBase+11)
      StopDrawing()
    EndIf
    
    ; --------- first button image
    gnNextImageNo + 1
    If n = 0
      nImageNo = hTrFirstEn13
    ElseIf n = 1
      nImageNo = hTrFirstDi13
    Else
      nImageNo = hTrFirstMo13
    EndIf
    If IsImage(nImageNo)
      FreeImage(nImageNo)
    EndIf
    CreateImage(nImageNo,nWidth,nHeight)
    If StartDrawing(ImageOutput(nImageNo))
      IMG_mac_drawTrBtnBackground()
      Box(xBase+3,yBase+1,2,11)
      Plot(xBase+7,yBase+6)
      LineXY(xBase+8,yBase+5,xBase+8,yBase+7)
      LineXY(xBase+9,yBase+4,xBase+9,yBase+8)
      LineXY(xBase+10,yBase+3,xBase+10,yBase+9)
      LineXY(xBase+11,yBase+2,xBase+11,yBase+10)
      LineXY(xBase+12,yBase+1,xBase+12,yBase+11)
      StopDrawing()
    EndIf
    
  Next n
  
EndProcedure

Procedure IMG_checkForUseCameraIcon(pSubPtr)
  PROCNAMECS(pSubPtr)
  Protected bUseCamera
  Protected k
  
  If pSubPtr >= 0
    k = aSub(pSubPtr)\nFirstAudIndex
    If k >= 0
      ; at least one aud present
      bUseCamera = #True
      While k >= 0
        If aAud(k)\nFileFormat = #SCS_FILEFORMAT_VIDEO
          bUseCamera = #False
          Break
        EndIf
        k = aAud(k)\nNextAudIndex
      Wend
    EndIf
  EndIf
  ProcedureReturn bUseCamera
EndProcedure

Procedure IMG_checkForUseLiveInputOffIcon(pSubPtr)
  PROCNAMECS(pSubPtr)
  Protected bUseLiveInputOff
  Protected k
  
  If pSubPtr >= 0
    k = aSub(pSubPtr)\nFirstAudIndex
    If k >= 0
      If (aAud(k)\nInputOnCount = 0) And (aAud(k)\nInputOffCount > 0)
        bUseLiveInputOff = #True
      EndIf
    EndIf
  EndIf
  ProcedureReturn bUseLiveInputOff
EndProcedure

Procedure IMG_getSubTypeImageHandle(pSubPtr, iSize = 24)
  ; This procedure will return the Image in 16x16 or 24x24 for a SubCue
  Protected nImageHandle
  
  If pSubPtr >= 0
    With aSub(pSubPtr)
      If iSize = 24
        ; size 24x24
        If \bSubTypeA
          If IMG_checkForUseCameraIcon(pSubPtr)
            nImageHandle = hCueTypeCamera
          Else
            nImageHandle = hCueTypeVideo
          EndIf
        ElseIf \bSubTypeQ
          nImageHandle = hCueTypeCallCue
        ElseIf \bSubTypeE
          nImageHandle = hCueTypeMemo
        ElseIf \bSubTypeF
          nImageHandle = hCueTypeAudio
        ElseIf \bSubTypeG
          nImageHandle = hCueTypeGoToCue
        ElseIf \bSubTypeI
          If IMG_checkForUseLiveInputOffIcon(pSubPtr)
            nImageHandle = hCueTypeLiveInputOff
          Else
            nImageHandle = hCueTypeLiveInput
          EndIf
        ElseIf \bSubTypeJ
          nImageHandle = hCueTypeEnaDis
        ElseIf \bSubTypeK
          nImageHandle = hCueTypeLighting
        ElseIf \bSubTypeL
          nImageHandle = hCueTypeLvlChg
        ElseIf \bSubTypeM
          nImageHandle = hCueTypeCtrlSend
        ElseIf \bSubTypeN
          nImageHandle = hCueTypeNote
        ElseIf \bSubTypeP
          nImageHandle = hCueTypePlaylist
        ElseIf \bSubTypeR
          nImageHandle = hCueTypeRun
        ElseIf \bSubTypeS
          nImageHandle = hCueTypeSFR
        ElseIf \bSubTypeT
          nImageHandle = hCueTypeSetPos
        ElseIf \bSubTypeU
          nImageHandle = hCueTypeMTC
        EndIf
      Else
        ; size 16x16      
        If \bSubTypeA
          nImageHandle = hClVideo
        ElseIf \bSubTypeE
          nImageHandle = hClMemo
        ElseIf \bSubTypeF
          nImageHandle = hClAudio
        ElseIf \bSubTypeG
          nImageHandle = hClGoToCue
        ElseIf \bSubTypeI
          nImageHandle = hClLiveInput
        ElseIf \bSubTypeJ
          nImageHandle = hClEnaDis
        ElseIf \bSubTypeK
          nImageHandle = hClLighting
        ElseIf \bSubTypeL
          nImageHandle = hClLvlChg
        ElseIf \bSubTypeM
          nImageHandle = hClCtrlSend
        ElseIf \bSubTypeN
          nImageHandle = hClNote
        ElseIf \bSubTypeP
          nImageHandle = hClPlaylist
        ElseIf \bSubTypeQ
          nImageHandle = hClCallCue
        ElseIf \bSubTypeR
          nImageHandle = hClRun
        ElseIf \bSubTypeS
          nImageHandle = hClSFR
        ElseIf \bSubTypeT
          nImageHandle = hClSetPos
        ElseIf \bSubTypeU
          nImageHandle = hClMTC  
        EndIf
      EndIf
      
    EndWith
  EndIf
  
  ProcedureReturn nImageHandle
EndProcedure


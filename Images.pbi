Procedure IMG_GreyScale(nSourceImageNo, nLightenFactor=0)
  ; nLightenFactor included as an optional parameter as sometimes a 'grey' image can be too dark against a black or near-black background.
  ; Suggested value if used: 20
  ; See also IMG_LightenColors()
  Protected nTargetImageNo, nWidth, nHeight, x, y, nPixel, nAlpha, nGray
  
  If IsImage(nSourceImageNo)
    gnNextImageNo + 1
    nTargetImageNo = gnNextImageNo
    If CopyImage(nSourceImageNo, nTargetImageNo)
      StartDrawing(ImageOutput(nTargetImageNo))
      DrawingMode(#PB_2DDrawing_AlphaBlend)
      nWidth = ImageWidth(nTargetImageNo)
      nHeight = ImageHeight(nTargetImageNo)
      For y = 0 To nHeight-1
        For x = 0 To nWidth-1
          nPixel = Point(x, y)
          nGray = Red(nPixel) * 0.2989
          nGray + Green(nPixel) * 0.5870
          nGray + Blue(nPixel) * 0.1140
          If nLightenFactor <> 0
            nGray + nLightenFactor
            If nGray < 0 : nGray = 0 : ElseIf nGray > 255 : nGray = 255 : EndIf
          EndIf
          nAlpha = Alpha(nPixel)
          Plot(x, y, RGBA(nGray, nGray, nGray, nAlpha))
        Next x
      Next y
      StopDrawing()
    Else
      nTargetImageNo = 0
    EndIf
  EndIf
  ProcedureReturn nTargetImageNo
  
EndProcedure

Macro IMG_doCatchImage(pHandle, pImage)
  ; See also Macro IMG_doCatchImagePlusGreyImage()
  If IsImage(pHandle) = #False
    gnNextImageNo + 1
    pHandle = gnNextImageNo
    If CatchImage(pHandle, ?pImage) = 0
      debugMsg0(sProcName, "CatchImage failed")
    EndIf
  EndIf
EndMacro

Macro IMG_doCatchImagePlusGreyImage(pHandleEn, pImageEn, pHandleDi, nLightenFactor=-1)
  ; See also Macro IMG_doCatchImage()
  If IsImage(pHandleEn) = #False
    gnNextImageNo + 1
    pHandleEn = gnNextImageNo
    If CatchImage(pHandleEn, ?pImageEn) = 0
      debugMsg0(sProcName, "CatchImage failed")
    Else
      pHandleDi = IMG_GreyScale(pHandleEn, nLightenFactor)
    EndIf
  EndIf
EndMacro


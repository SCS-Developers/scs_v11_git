Macro IMG_mac_startAlphaDrawing(hImage)
  gnNextImageNo + 1
  hImage = gnNextImageNo
  CreateImage(hImage, nWidth, nHeight, 32)
  StartDrawing(ImageOutput(hImage))
  DrawingMode(#PB_2DDrawing_AlphaChannel)                     ; need these two lines to make
  Box(0, 0, OutputWidth(), OutputHeight(), RGBA(0, 0, 0, 0))  ; the whole background transparent
  DrawingMode(#PB_2DDrawing_AlphaBlend)
EndMacro

Procedure IMG_LightenColors(nImageNo, nLightenFactor=-1)
  Protected nWidth, nHeight, x, y, nPixel, nAlpha, nRed, nGreen, nBlue, nReqdLightenFactor
  
  If IsImage(nImageNo)
    If nLightenFactor = -1
      nReqdLightenFactor = 20
    Else
      nReqdLightenFactor = nLightenFactor ; nb may be zero
    EndIf
    StartDrawing(ImageOutput(nImageNo))
    DrawingMode(#PB_2DDrawing_AlphaBlend)
    nWidth = ImageWidth(nImageNo)
    nHeight = ImageHeight(nImageNo)
    For y = 0 To nHeight-1
      For x = 0 To nWidth-1
        nPixel = Point(x, y)
        nAlpha = Alpha(nPixel)
        If nAlpha > 0
          nRed = Red(nPixel) + nReqdLightenFactor
          nGreen = Green(nPixel) + nReqdLightenFactor
          nBlue = Blue(nPixel) + nReqdLightenFactor
          If nRed < 0 : nRed = 0 : ElseIf nRed > 255 : nRed = 255 : EndIf
          If nGreen < 0 : nGreen = 0 : ElseIf nGreen > 255 : nGreen = 255 : EndIf
          If nBlue < 0 : nBlue = 0 : ElseIf nBlue > 255 : nBlue = 255 : EndIf
          Plot(x, y, RGBA(nRed, nGreen, nBlue, nAlpha))
        EndIf
      Next x
    Next y
    StopDrawing()
  EndIf
  
EndProcedure


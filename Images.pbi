Procedure IMG_GetRightmostColumn(nImageNo)
  Protected nWidth, nHeight, x, y, nRightMostColumn
  
  If IsImage(nImageNo)
    If StartDrawing(ImageOutput(nImageNo))
      nWidth = ImageWidth(nImageNo)
      nHeight = ImageHeight(nImageNo)
      DrawingMode(#PB_2DDrawing_AlphaChannel) ; only interested in the alpha channel for this procedure
      For x = nWidth-1 To 0 Step -1 ; work backwards from RHS
        For y = 0 To nHeight-1
          If Alpha(Point(x, y)) > 0
            ; non-transparent pixel found in this column (ie in a pixel in this 'x')
            nRightMostColumn = x
            Break 2 ; Break y, x
          EndIf
        Next y
      Next x
      StopDrawing()
    EndIf
  EndIf
  ProcedureReturn nRightMostColumn
  
EndProcedure

Procedure IMG_buildAddImage(hClImage, sType.s, bEnabled=#True)
  ; PROCNAMEC()
  Protected hNewImage, hAddImage, nRightMostColumn, nLeft
  
  Select sType
    Case "Q"
      If bEnabled
        hAddImage = hToolAddQEn
      Else
        hAddImage = hToolAddQDi
      EndIf
    Case "S"
      If bEnabled
        hAddImage = hToolAddSEn
      Else
        hAddImage = hToolAddSDi
      EndIf
  EndSelect
  If IsImage(hClImage)
    nRightMostColumn = IMG_GetRightmostColumn(hClImage)
    gnNextImageNo + 1
    hNewImage = gnNextImageNo
    CreateImage(hNewImage, 27, 24, 32) ; must be 32-bit to include alpha channel
    If StartDrawing(ImageOutput(hNewImage))
      DrawingMode(#PB_2DDrawing_AlphaChannel)                     ; need these two lines to make
      Box(0, 0, OutputWidth(), OutputHeight(), RGBA(0, 0, 0, 0))  ; make the whole background transparent
      DrawingMode(#PB_2DDrawing_AlphaBlend)
      Select ImageWidth(hClImage)
        Case 16
          ; a 16x16 'cue list' image
          DrawAlphaImage(ImageID(hClImage),11,4)
        Case 24
          ; a 24x24 'cue type' image
          nLeft = 27 - nRightMostColumn
          ; debugMsg(sProcName, "nRightMostColumn=" + nRightMostColumn + ", nLeft=" + nLeft)
          DrawAlphaImage(ImageID(hClImage), nLeft, 0)
      EndSelect
      DrawAlphaImage(ImageID(hAddImage), 0, 0) ; draw this AFTER drawing the cue icon so that it is in front of any overlapping part of the cue icon
      StopDrawing()
    EndIf
  EndIf
  ProcedureReturn hNewImage
EndProcedure

